pipeline{
    agent any
    stages{
        stage("Printing Hello World"){
            steps{
                echo 'Hello World'
            }
        }
        stage("Printing Happy Coding"){
            steps{
                echo 'Happy Coding!!'
            }
        }
        stage("Printing Happy New Year 2023"){
            steps{
                echo 'Happy New Year 2023!!'
            }
        }
    }
}